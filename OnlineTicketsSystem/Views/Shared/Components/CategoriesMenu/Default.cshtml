    @model IEnumerable<OnlineTicketsSystem.Models.Category>

    @foreach (var category in Model)
    {
        <li>
            <a class="dropdown-item"
               asp-controller="Events"
               asp-action="Index"
               asp-route-category="@category.Name">
                @category.Name
            </a>
        </li>
    }

